<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:inset="@dimen/keyInset">

    <layer-list>
        <item>
            <shape android:shape="rectangle">
                <corners android:radius="5dp" />
                <gradient
                    android:angle="0"
                    android:endColor="#FF7F00"
                    android:startColor="#FF0000" />
            </shape>
        </item>

        <item
            android:bottom="@dimen/theme02stroke"
            android:left="@dimen/theme02stroke"
            android:right="@dimen/theme02stroke"
            android:top="@dimen/theme02stroke">
            <shape android:shape="rectangle">
                <corners android:radius="5dp" />
                <solid
                    android:color="#000000" />
            </shape>
        </item>
    </layer-list>
</inset>