<?xml version="1.0" encoding="utf-8"?>
<inset android:inset="5dp"
    xmlns:android="http://schemas.android.com/apk/res/android">
<layer-list>
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="5dp" />
            <solid
                android:color="#FFF3E0"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="4dp" />
            <stroke
                android:color="#FFD187"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>
</inset>