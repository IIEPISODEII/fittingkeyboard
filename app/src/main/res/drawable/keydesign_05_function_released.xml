<?xml version="1.0" encoding="utf-8"?>
<inset android:inset="@dimen/keyInset"
    xmlns:android="http://schemas.android.com/apk/res/android">
<layer-list>
    <item android:bottom="0dp">
        <shape android:shape="rectangle">
            <corners
                android:radius="5dp" />
            <solid
                android:color="#D6B3FF" />
        </shape>
    </item>

    <item android:top="@dimen/keyInset"
        android:bottom="@dimen/keyInset"
        android:left="@dimen/keyInset"
        android:right="@dimen/keyInset">
        <shape android:shape="rectangle">
            <corners
                android:radius="4dp" />
            <stroke
                android:color="@color/white"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>
</inset>
